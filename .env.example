# Environment Configuration
NODE_ENV=development

# iMessage SDK Configuration
IMESSAGE_DEBUG=true
IMESSAGE_MAX_CONCURRENT=5

# Google Calendar API (OAuth 2.0)
# Get these from https://console.cloud.google.com/
GOOGLE_CALENDAR_CLIENT_ID=""
GOOGLE_CALENDAR_CLIENT_SECRET=""
GOOGLE_CALENDAR_REDIRECT_URI="http://localhost:3000/oauth/callback"
GOOGLE_CALENDAR_ID="primary"

# Google Calendar Access Tokens (from OAuth setup)
GOOGLE_CALENDAR_ACCESS_TOKEN=""
GOOGLE_CALENDAR_REFRESH_TOKEN=""


# OpenAI API
OPENAI_API_KEY=your_openai_api_key

# Database
DATABASE_PATH=./data/coffee_network.db

# Database (optional - uses default path if not specified)
DATABASE_PATH=./data/coffee_network.db