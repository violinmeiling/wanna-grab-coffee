# Environment Configuration
NODE_ENV=development

# iMessage SDK Configuration
IMESSAGE_DEBUG=true
IMESSAGE_MAX_CONCURRENT=5

# Google Calendar API
GOOGLE_CALENDAR_CLIENT_ID=your_google_client_id
GOOGLE_CALENDAR_CLIENT_SECRET=your_google_client_secret
GOOGLE_CALENDAR_REDIRECT_URI=http://localhost:3000/oauth/callback
GOOGLE_CALENDAR_ID=primary

# Google Gemini AI
GOOGLE_AI_API_KEY=your_gemini_api_key

# LinkedIn (if using API integration)
LINKEDIN_ACCESS_TOKEN=your_linkedin_token

# Database
DATABASE_PATH=./data/coffee_network.db

# Your personal info for context
MY_NAME=Your Name
MY_PHONE=+1234567890

# Scheduling
DEFAULT_COFFEE_DURATION_MINUTES=60
BUSINESS_HOURS_START=9
BUSINESS_HOURS_END=19

# Webhook (optional)
WEBHOOK_URL=
WEBHOOK_SECRET=